{
    "name": "libero/api-client-bundle",
    "description": "Libero API client",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Libero\\ApiClientBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "guzzlehttp/promises": "^1.3",
        "league/flysystem": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.4",
        "phpunit/phpunit": "^7.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
